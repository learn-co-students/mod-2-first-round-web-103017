<h1>Create Superhero: </h1>
<%= form_for @superhero do |f| %>
  <h3><%= f.label :name %>:</h3>
  <%= f.text_field :name %><br />

  <h3><%= f.label :super_name %>:</h3>
  <%= f.text_field :super_name %><br />

	<h3><%= f.label :superpower %>:</h3>
	<%= f.collection_select(:superpower_id, @superpowers, :id, :name)%><br />
	
	<br />
  <%= f.submit %>
<% end %>
