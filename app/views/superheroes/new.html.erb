<h1>Create a Superhero</h1>


<ul>
<%= form_for @superhero do |f|%>

<%= f.label :name %>
<%= f.text_field :name%>
<br>
<%= f.label :super_name%>
<%= f.text_field :super_name%>
<br>
<%= f.label :superpower%>
<%= f.collection_select(:superpower_id, @superpowers.all, :id, :name) %>
<br>
<%= f.submit%>
<%end%>


</ul>
